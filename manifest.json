{
  "manifest_version": 2,
  "name": "Back to Top Button",
  "version": "1.0",
  "description": "Adds a 'Back to Top' button on every page.",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ]
}